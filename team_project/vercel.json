{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/checkDuplicate",
            "dest": "/api/index.js"
        },
        {
            "src": "/sendCode",
            "dest": "/api/index.js"
        },
        {
            "src": "/verifyCode",
            "dest": "/api/index.js"
        },
        {
            "src": "/saveStep1",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/views/$1"
        }
    ]
}